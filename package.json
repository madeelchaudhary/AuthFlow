{
  "name": "authflow",
  "description": "AuthFlow is a fullstack authentication flow built with customizability in mind.",
  "keywords": [
    "nextjs",
    "authentication",
    "jwt",
    "authorization"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types.d.ts",
  "license": "MIT",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build": "yarn clean && yarn lint && yarn tsup",
    "test": "jest",
    "prepublishOnly": "yarn build",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "bcrypt": "^5.1.1",
    "jose": "^5.2.4",
    "react-hook-form": "^7.51.3",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "autoprefixer": "^10.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^8",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lucide-react": "^0.371.0",
    "next": "^14",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/idex.mjs",
      "require": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./withAuth": {
      "import": "./dist/withAuth.mjs",
      "require": "./dist/withAuth.js",
      "types": "./dist/withAuth.d.ts"
    },
    "./withAuthEdge": {
      "import": "./dist/withAuthEdge.mjs",
      "require": "./dist/withAuthEdge.js",
      "types": "./dist/withAuthEdge.d.ts"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  }
}
