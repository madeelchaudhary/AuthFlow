{
  "name": "authflow-next",
  "description": "AuthFlow is a fullstack authentication flow built with customizability in mind.",
  "keywords": [
    "nextjs",
    "authentication",
    "jwt",
    "authorization"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types.d.ts",
  "license": "MIT",
  "version": "0.1.0",
  "author": {
    "email": "madeelchaudhry@gmail.com",
    "name": "M Adeel",
    "url": "https://github.com/madeelchaudhary"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "publish": "yarn lint && yarn build && changeset publish",
    "prepublish": "yarn build",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write",
    "clean": "rm -rf dist",
    "tailwind:css": "tailwindcss -i ./src/react/globals.css -o ./dist/tailwind.css"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "bcrypt": "^5.1.1",
    "jose": "^5.2.4",
    "react-hook-form": "^7.51.3",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "autoprefixer": "^10.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^8",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "next": "^14",
    "postcss": "^8",
    "prettier": "3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.17.0",
    "yarn": ">=1.22.22"
  },
  "packageManager": "yarn@1.22.22",
  "repository": {
    "type": "git",
    "url": "https://github.com/madeelchaudhary/AuthFlow"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./withAuth": {
      "import": "./dist/withAuth.mjs",
      "require": "./dist/withAuth.js"
    },
    "./withAuthEdge": {
      "import": "./dist/withAuthEdge.mjs",
      "require": "./dist/withAuthEdge.js"
    },
    "./index.css": "./dist/react/index.css",
    "./types": "./dist/types.d.ts"
  }
}
